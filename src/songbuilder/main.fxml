<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="songbuilder.MainController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="300.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="75.0" vgrow="NEVER" />
   </rowConstraints>
   <children>
    <ScrollPane id="paneMain" fitToWidth="TRUE">
      <content>
        <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="200.0" />
      </content>
    </ScrollPane>
    <ScrollPane id="paneVerses" GridPane.columnIndex="1" GridPane.rowSpan="2" fitToWidth="TRUE">
      <content>
        <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="200.0"/>
      </content>
    </ScrollPane>
      <AnchorPane id="panePlayer" GridPane.rowIndex="1" >
         <children>
          <Button id="buttonPlay" prefWidth="32" AnchorPane.topAnchor="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="15.0" mnemonicParsing="false" styleClass="playButton" />
          <Slider id="sliderPlayProgress" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="60.0" />
          <Label id="labelPlayProgress" AnchorPane.topAnchor="20.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" text="00:00 / 03:45" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
